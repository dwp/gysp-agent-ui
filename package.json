{
  "name": "agent-ui",
  "version": "5.19.0",
  "snapshot": "9.9.9-SNAPSHOT",
  "repository": {
    "type": "git",
    "url": "git@github.com:dwp/gysp-agent-ui.git"
  },
  "engines": {
    "node": "10.17.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "version": "./postversion.sh",
    "build": "gulp generateAssets",
    "package": "gulp --gulpfile ./gulp-package.js",
    "release": "npm install && npm test && npm run build && npm prune --production && npm run package && npm install",
    "start": "node start | bunyan",
    "lint": "eslint .",
    "test": "CONFIG_LOCATION='./test/resource/' mocha test/unit/**/**",
    "test-coverage": "CONFIG_LOCATION='./test/resource/' BANK_FEATURE=false nyc mocha --reporter=mocha test/unit --recursive ",
    "security-tests": "npm run security:outdated && npm run security:audit",
    "security:audit": "npm audit || true",
    "security:snyk": "snyk test || true",
    "security:outdated": "npm outdated || true",
    "compliance-tests": "npm run lint",
    "ci": "npm run compliance-tests && npm run compliance-tests && npm run test && npm run test-coverage"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "0.0.17-alpha",
    "body-parser": "1.19.0",
    "bunyan": "1.8.12",
    "bunyan-rotating-file-stream": "1.6.3",
    "compression": "1.7.4",
    "connect-redis": "4.0.3",
    "crypto-js": "3.1.9-1",
    "debug": "4.1.1",
    "express": "4.17.1",
    "express-flash": "0.0.2",
    "express-session": "1.17.0",
    "govuk-frontend": "3.5.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.2.0",
    "gulp-concat": "2.6.1",
    "gulp-nodemon": "2.4.2",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sass-lint": "1.4.0",
    "gulp-tar": "3.1.0",
    "gulp-uglify": "3.0.2",
    "helmet": "3.21.2",
    "http-status-codes": "1.4.0",
    "i18next": "1.10.6",
    "itemid": "1.1.0",
    "js-yaml": "3.13.1",
    "mockdate": "2.0.5",
    "moment": "2.24.0",
    "nunjucks": "3.2.0",
    "redis": "2.8.0",
    "request-promise": "4.2.5",
    "serve-favicon": "2.5.0",
    "validator": "12.1.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/travis-cli": "8.2.0",
    "@dwp/commitlint-config-base": "1.1.0",
    "@dwp/eslint-config-base": "4.1.0",
    "@dwp/eslint-config-mocha": "1.0.0",
    "aws-sdk": "2.556.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "conventional-changelog-cli": "2.0.28",
    "del": "5.1.0",
    "eslint": "6.7.2",
    "html5shiv": "3.7.3",
    "husky": "3.1.0",
    "jquery": "3.4.1",
    "jquery-autotab": "1.9.2",
    "mocha": "6.2.2",
    "nock": "11.7.0",
    "nyc": "14.1.1",
    "snyk": "1.259.0"
  }
}

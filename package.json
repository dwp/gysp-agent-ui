{
  "name": "agent-ui",
  "version": "5.7.0",
  "snapshot": "9.9.9-SNAPSHOT",
  "repository": {
    "type": "git",
    "url": "git@github.com:dwp/gysp-agent-ui.git"
  },
  "engines": {
    "node": "10.15.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "version": "./postversion.sh",
    "build": "gulp generateAssets",
    "package": "gulp --gulpfile ./gulp-package.js",
    "release": "npm install && npm test && npm run build && npm prune --production && npm run package && npm install",
    "prestart": "npm install",
    "start": "node start",
    "lint": "eslint .",
    "test": "CONFIG_LOCATION='./test/resource/' mocha test/unit/**/**",
    "test-coverage": "CONFIG_LOCATION='./test/resource/' BANK_FEATURE=false nyc mocha --reporter=mocha test/unit --recursive ",
    "security-tests": "npm run security:outdated && npm run security:audit",
    "security:audit": "npm audit || true",
    "security:snyk": "snyk test || true",
    "security:outdated": "npm outdated || true",
    "compliance-tests": "npm run lint"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "bunyan": "1.8.12",
    "bunyan-rotating-file-stream": "1.6.3",
    "compression": "1.7.4",
    "connect-redis": "3.4.1",
    "crypto-js": "3.1.9-1",
    "express": "4.16.4",
    "express-flash": "0.0.2",
    "express-session": "1.16.1",
    "govuk-frontend": "2.11.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "3.9.4",
    "gulp-concat": "2.6.1",
    "gulp-nodemon": "2.4.2",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-sass-lint": "1.4.0",
    "gulp-tar": "2.1.0",
    "gulp-uglify": "3.0.2",
    "helmet": "3.16.0",
    "http-status-codes": "1.3.2",
    "i18next": "1.10.6",
    "itemid": "1.1.0",
    "js-yaml": "3.13.1",
    "moment": "2.24.0",
    "nunjucks": "3.2.0",
    "request-promise": "3.0.0",
    "serve-favicon": "2.5.0",
    "validator": "10.11.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/travis-cli": "7.6.0",
    "@dwp/commitlint-config-base": "1.0.0",
    "@dwp/eslint-config-base": "2.1.0",
    "@dwp/eslint-config-mocha": "1.0.0",
    "aws-sdk": "2.451.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "conventional-changelog-cli": "2.0.21",
    "del": "4.1.1",
    "eslint": "5.16.0",
    "html5shiv": "3.7.3",
    "husky": "1.3.1",
    "jquery": "3.4.0",
    "jquery-autotab": "1.9.2",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "nyc": "14.0.0",
    "snyk": "1.162.0"
  }
}

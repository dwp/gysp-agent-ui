{
  "name": "agent-ui",
  "version": "8.2.0",
  "description": "Agent frontend for Get your State Pension",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:dwp/gysp-agent-ui.git"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": "6.14.11"
  },
  "scripts": {
    "build": "gulp generateAssets",
    "prestart": "npm ci",
    "start": "gulp | bunyan",
    "compliance": "npm run compliance:lint && npm run compliance:lint-docker",
    "compliance:lint": "eslint .",
    "compliance:lint-docker": "docker run --rm -i hadolint/hadolint < docker/Dockerfile",
    "test": "npm run test:unit",
    "test:unit": "mocha test/unit --recursive",
    "quality": "npm run quality:coverage",
    "quality:coverage": "nyc mocha --reporter=mocha test/unit --recursive",
    "security": "npm run security:audit && npm run security:outdated",
    "security:audit": "npm audit || true",
    "security:outdated": "npm outdated || true",
    "security:snyk": "snyk test || true"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "0.2.1",
    "body-parser": "1.19.0",
    "bunyan": "1.8.15",
    "compression": "1.7.4",
    "connect-redis": "5.1.0",
    "crypto-js": "4.0.0",
    "debug": "4.3.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-flash": "0.0.2",
    "express-session": "1.17.1",
    "govuk-frontend": "3.11.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.3.0",
    "gulp-concat": "2.6.1",
    "gulp-nodemon": "2.5.0",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sass-lint": "1.4.0",
    "gulp-tar": "3.1.0",
    "gulp-uglify": "3.0.2",
    "helmet": "4.4.1",
    "http-status-codes": "2.1.4",
    "https": "1.0.0",
    "i18next": "19.8.7",
    "i18next-fs-backend": "1.0.8",
    "i18next-http-middleware": "3.1.0",
    "itemid": "1.1.0",
    "mockdate": "3.0.2",
    "moment": "2.29.1",
    "nocache": "2.1.0",
    "nunjucks": "3.2.3",
    "prom-client": "13.1.0",
    "redis": "3.0.2",
    "request-promise": "4.2.6",
    "response-time": "2.3.2",
    "serve-favicon": "2.5.0",
    "validator": "13.5.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/travis-cli": "11.0.0",
    "@dwp/commitlint-config-base": "1.1.0",
    "@dwp/eslint-config-base": "4.1.0",
    "@dwp/eslint-config-mocha": "1.1.0",
    "chai": "4.3.0",
    "chai-as-promised": "7.1.1",
    "conventional-changelog-cli": "2.1.1",
    "del": "6.0.0",
    "eslint": "7.20.0",
    "html5shiv": "3.7.3",
    "husky": "4.3.8",
    "jquery": "3.5.1",
    "jquery-autotab": "1.9.2",
    "mocha": "8.3.0",
    "nock": "13.0.7",
    "nyc": "15.1.0",
    "snyk": "1.454.0"
  }
}
